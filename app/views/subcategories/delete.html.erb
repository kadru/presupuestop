<%= fields model: category do |form| %>
  <%= form.fields_for :subcategories, child_index: index do |subcategories_form| %>
     <%= turbo_frame_tag "subcategory_#{subcategories_form.index}" do %>
       <%= subcategories_form.hidden_field :id, value: id %>
       <%= subcategories_form.hidden_field :_destroy, value: true %>
     <% end %>
  <% end %>
<% end %>
